{
  "name": "cubby-remote-reaper",
  "version": "1.1.0",
  "author": {
    "name": "Willard Jansen",
    "email": "willardjansen@me.com"
  },
  "description": "Articulation switcher and template builder for Reaper with Reaticulate",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "next dev --hostname 0.0.0.0 --port 7100",
    "build": "next build",
    "start": "next start --hostname 0.0.0.0 --port 7100",
    "lint": "next lint",
    "midi": "node midi-server.js",
    "all": "npm run midi & npm run dev",
    "electron:dev": "concurrently -k \"npm run dev\" \"wait-on http://localhost:7100 && electron .\"",
    "electron:build": "node build-electron.js",
    "electron:pack": "node build-electron.js --dir",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "jzz": "^1.9.6",
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "wait-on": "^7.2.0"
  }
}
